<resources>
    <style name="AppTheme" parent="AppBaseTheme"> </style>
	<style name="popwin_anim_style">  
     <item name="android:windowEnterAnimation">@anim/menushow</item>  
     <item name="android:windowExitAnimation">@anim/menuhide</item>  
	</style> 
	 <style name="MyLightTheme" parent="AppBaseTheme">
        <item name="background">#eeeeee</item>
        <item name="dividebackground"> #88999999</item> 
        <item name="textSize">16sp</item>
        <item name="textColor"> #111111</item>
        <item name="rlbackground"> #ffffff</item>   
       <item name="titlebackground"> #ebebeb</item>  
        <item name="textColor_selected"> @color/blue</item> 
    </style>
    <style name="MyNightTheme" parent="AppBaseTheme">
        <item name="background">#222222</item>
        <item name="dividebackground"> #88626262</item> 
        <item name="textSize">16sp</item>
        <item name="textColor"> #ffffff</item>  
        <item name="rlbackground"> #111111</item>  
        <item name="titlebackground"> #222222</item> 
         <item name="textColor_selected">@color/blue</item>    
    </style>
</resources>
